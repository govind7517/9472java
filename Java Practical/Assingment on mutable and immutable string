//IMMUTABLE STRING

public final class ImmutableStringDemo {
	
	private String name;
	
	ImmutableStringDemo(String name){
		this.name=name;
	}
	public String getName() {
		return name;
	}
	

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		ImmutableStringDemo obj = new ImmutableStringDemo("Hello");
		
		System.out.println(obj.getName());
		
		//obj.setName("new Hello");
		
		//System.out.println(obj.setName());	
		
		

	}

}


//MUTABLE STRING

public class MutableDemo {
	
	private String name;
	
	MutableDemo(String name){
		
		this.name =name;
	}
	
	public String getName() {
		return name;
	}
	
	public void setName(String name) {
		this.name =name;
		//return name;in place of void string use .
		
	}

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		MutableDemo obj = new MutableDemo("hello2");
		System.out.println(obj.getName());
		
		obj.setName("new hello2");
		System.out.println(obj.getName());

		

	}

}
